{
  "name": "ecom",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "heroku-postinstall": "npm install && npm run serve"
  },
  "devServer": {
    "disableHostCheck": true
  },
  "dependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.5",
    "@vue/cli-service": "^4.0.0",
    "aws-cli-js": "^2.0.6",
    "aws-sdk": "^2.578.0",
    "babel-eslint": "^10.0.3",
    "body-parser": "^1.19.0",
    "core-js": "^3.3.2",
    "dns": "^0.2.2",
    "ejs": "latest",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "express": "^4.17.1",
    "hooper": "^0.3.4",
    "net": "^1.0.2",
    "node-ses": "^2.2.1",
    "nodemailer": "^6.3.1",
    "vue": "^2.6.10",
    "vue-inject": "^2.1.1",
    "vue-router": "^3.1.3",
    "vue-template-compiler": "^2.6.10",
    "xoauth2": "^1.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-plugin-router": "^4.0.5",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": 1
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "vue/no-unused-components": [
    "error",
    {
      "ignoreWhenBindingPresent": true
    }
  ]
}
